<mvc:View
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    displayBlock="true"
    controllerName="masterdetailgs.masterdetailgs.controller.GameDetails">
    <Page id="_IDGenPage1" title="Game Details">
        <content>
            <VBox id="_IDGenVBox1" class="sapUiSmallMargin">
                <Text id="_IDGenText1" text="{Name}" class="sapUiSmallMarginBottom"/>
                <Text id="_IDGenText2" text="{Genre}" class="sapUiSmallMarginBottom"/>
                <Text id="_IDGenText3" text="{Publisher}" class="sapUiSmallMarginBottom"/>
                <Text id="_IDGenText4" text="{Platform}" class="sapUiSmallMarginBottom"/>
                <Text id="_IDGenText5" text="{Releasedate}" class="sapUiSmallMarginBottom"/>
                <Text id="_IDGenText6" text="{Coverurl}" class="sapUiSmallMarginBottom"/>
                <Text id="_IDGenText7" text="{Description}" class="sapUiSmallMarginBottom"/>
                <Text id="_IDGenText8" text="Favorited by students:" class="sapUiSmallMarginBottom"/>
                <List id="_IDGenList1" items="{FavGames}">
                    <StandardListItem id="_IDGenStandardListItem1" title="{Studentid}"/>
                </List>
            </VBox>
        </content>
    </Page>
</mvc:View>
