<mvc:View
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    displayBlock="true"
    controllerName="masterdetailgs.masterdetailgs.controller.Games">
    <Page id="page" title="{i18n>gamesTitle}">
        <content>
            <List id="gameList" items="{/GameSet}" mode="SingleSelectMaster" itemPress="onItemPress">
                <ObjectListItem id="_IDGenObjectListItem1"
                    title="{Name}"
                    number="{Id}"
                    numberUnit="{Genre}"
                    type="Active">
                    <attributes>
                        <ObjectAttribute id="_IDGenObjectAttribute1" text="{Publisher}"/>
                        <ObjectAttribute id="_IDGenObjectAttribute2" text="{Platform}"/>
                        <ObjectAttribute id="_IDGenObjectAttribute3" text="{Releasedate}"/>
                    </attributes>
                </ObjectListItem>
            </List>
        </content>
    </Page>
</mvc:View>
